<?xml version="1.0" encoding="utf-8"?>
<resources>
  <style name="IconButton">
    <item name="iconPadding">0dp</item>
    <item name="android:insetLeft">0dp</item>
    <item name="android:insetTop">0dp</item>
    <item name="android:insetRight">0dp</item>
    <item name="android:insetBottom">0dp</item>
    <item name="android:padding">0dp</item>
    <item name="iconGravity">textStart</item>
    <item name="android:stateListAnimator">@null</item>
  </style>

  <style name="BottomBarIconButton" parent="IconButton">
    <item name="iconTint">@color/purple_50p</item>
  </style>

  <style name="Button">
    <item name="strokeColor">@color/dark_purple</item>
    <item name="strokeWidth">2dp</item>
    <item name="android:textColor">@color/dark_purple</item>
    <item name="android:fontFamily">@font/poppins_bold</item>
    <item name="android:backgroundTint">@color/grey</item>
    <item name="cornerRadius">10dp</item>
  </style>
</resources>